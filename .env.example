# Cloud Provider API
CLOUD_API_KEY=your-cloud-api-key-here
CLOUD_REGION=us-east-1
CLOUD_PLAN=small-1cpu-1gb

# MongoDB
MONGO_URI=mongodb://localhost:27017/cloud-orchestration

# Provisioning Service
PROVISIONER_API_KEY=your-secure-random-key-here
CONTROL_API_URL=http://your-server-ip:4600
ADMIN_API_KEY=your-secure-admin-key-here
ADMIN_API_PORT=4310

# Scheduling
DESTROY_CONCURRENCY=30
DESTROY_LEASE_MS=120000
DESTROY_BUFFER_MS=60000
START_LOCK_MS=900000

# Timeouts
ADMIN_READY_TIMEOUT_MS=120000
ADMIN_START_RETRIES=6

# Server
PORT=4600
NODE_ENV=development
